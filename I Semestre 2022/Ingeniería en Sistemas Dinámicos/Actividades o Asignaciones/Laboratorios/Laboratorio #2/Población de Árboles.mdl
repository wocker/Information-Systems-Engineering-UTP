Deforestación=
	IF THEN ELSE(Población de Árboles > 75000 , Producción Nacional , 0 )
	~	
	~		|

Incendios Forestales=
	(Varianza de Incendios*Población de Árboles)/100
	~	
	~		|

Población de Árboles= INTEG (
	Reforestación-Deforestación-Incendios Forestales,
		100000)
	~	{1000}
	~		|

Producción Nacional=
	RANDOM UNIFORM( 200 , 700 , 300 )
	~	
	~		|

Reforestación=
	Población de Árboles*Tasa de Reforestación
	~	
	~		|

Tasa de Reforestación=
	0.04
	~	
	~		|

Varianza de Incendios=
	RANDOM NORMAL(3 , 9 , 6 , 3 , 300 )
	~	
	~		|

